{
    "usesTypeScript": true,
    "usesCssModule": true,
    "cssPreprocessor": "scss",
    "testLibrary": "Testing Library",
    "component": {
        "default": {
            "customTemplates": {
                "component": "src/configs/templates/TemplateName.tsx",
                "style": "src/configs/templates/TemplateName.scss",
                "hook": "src/configs/templates/useTemplateName.ts",
                "styled": "src/configs/templates/styledTemplateName.ts"
            },
            "path": "src/components",
            "withStyle": false,
            "withTest": false,
            "withStory": false,
            "withLazy": false,
            "withHook": true,
            "withStyled": true
        },
        "page": {
            "customTemplates": {
                "component": "src/configs/templates/TemplateName.tsx",
                "style": "src/configs/templates/TemplateName.scss",
                "hook": "src/configs/templates/useTemplateName.ts",
                "styled": "src/configs/templates/styledTemplateName.ts"
            },
            "path": "src/pages",
            "withStyle": false,
            "withTest": false,
            "withStory": false,
            "withLazy": false,
            "withHook": true,
            "withStyled": true
        },
        "layout": {
            "customTemplates": {
                "component": "src/configs/templates/TemplateName.tsx",
                "style": "src/configs/templates/TemplateName.scss",
                "hook": "src/configs/templates/useTemplateName.ts",
                "styled": "src/configs/templates/styledTemplateName.ts"
            },
            "path": "src/layout",
            "withStyle": false,
            "withTest": false,
            "withStory": false,
            "withLazy": false,
            "withHook": true,
            "withStyled": true
        }
    }
}