{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "src/static/configs/templates/TemplateName.tsx",
        "style": "src/static/configs/templates/TemplateName.scss",
        "hook": "src/static/configs/templates/useTemplateName.ts"
      },
      "path": "src/components",
      "withStyle": true,
      "withHook": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "src/static/configs/templates/TemplateName.tsx",
        "style": "src/static/configs/templates/TemplateName.scss",
        "hook": "src/static/configs/templates/useTemplateName.ts"
      },
      "path": "src/pages",
      "withStyle": true,
      "withHook": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "layout": {
      "customTemplates": {
        "component": "src/static/configs/templates/TemplateName.tsx",
        "style": "src/static/configs/templates/TemplateName.scss",
        "hook": "src/static/configs/templates/useTemplateName.ts"
      },
      "path": "src/layout",
      "withStyle": true,
      "withHook": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}